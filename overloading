overloading----------------------------------

#include<iostream>
using namespace std;


int sum(int a, int b)
{
	cout<<"using function 2 arguments"<<endl;
	return a+b;
 } 
 
int sum (int a, int b, int c)
{
	cout<<"using 3 agruments"<<endl;
	return a+b+c;
 }
 
int area (int a)
{
	return a * a * a;
}  

int area (int l , int b, int h)
{
	return l * b * h;
}  

int main()
{
	int a;
	cout<<"the sum of 2 numbers:"<<sum(3,6)<<endl;
	cout<<"the sum of 3 numbers:"<<sum(3,6,6)<<endl;
	cout<<"the area of a cube :"<<area(3)<<endl;
	cout<<"the area of a cuboid:"<<area(3, 4,5)<<endl;
	return 0;
}
